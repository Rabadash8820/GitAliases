[alias]
  m = merge
  ma = merge --abort
  mc = merge --continue
  mcne = !GIT_SEQUENCE_EDITOR=true GIT_EDITOR=true git merge --continue
  md = merge dev
  me = merge --edit
  mff = merge --ff
  mffo = merge --ff-only
  mgs = merge --gpg-sign
  mh = merge -h
  mm = merge master
  mmne = !GIT_SEQUENCE_EDITOR=true GIT_EDITOR=true git merge master
  mmnenf = !GIT_SEQUENCE_EDITOR=true GIT_EDITOR=true git merge master --no-ff
  mnenf = !GIT_SEQUENCE_EDITOR=true GIT_EDITOR=true git merge --no-ff
  mnf = merge --no-ff
  mq = merge --quiet
  msq = merge --squash
  mv = merge --verbose
  