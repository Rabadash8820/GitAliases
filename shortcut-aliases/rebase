[alias]
  rb = rebase
  rba = rebase --abort
  rbc = rebase --continue
  rbdate = rebase --committer-date-is-author-date
  rbet = rebase --edit-todo
  rbh = rebase -h
  rbi = rebase --interactive
  rbq = rebase --quiet
  rbis = rebase --interactive --autostash
  rbs = rebase --autostash
  rbisq = rebase --interactive --autosquash
  rbisqs = rebase --interactive --autosquash --autostash
  rbsq = !GIT_SEQUENCE_EDITOR=true GIT_EDITOR=true git rebase --interactive --autosquash
  rbsqs = !GIT_SEQUENCE_EDITOR=true GIT_EDITOR=true git rebase --interactive --autosquash --autostash
  rbv = rebase --verbose
  