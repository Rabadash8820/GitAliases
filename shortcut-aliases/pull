[alias]
  pl = pull
  pla = pull --all
  plap = pull --all --prune
  plapr = pull --all --prune --rebase=true
  plaps = pull --all --prune --rebase=true --autostash
  plf = pull --force
  plh = pull -h
  plp = pull --prune
  plpr = pull --prune --rebase=true
  plps = pull --prune --rebase=true --autostash
  plr = pull --rebase=true
  plq = pull --quiet
  pls = pull --rebase=true --autostash
  plt = pull --tags
  plv = pull --verbose
  