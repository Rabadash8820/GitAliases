[alias]
  rb = rebase
  rba = rebase --abort
  rbc = rebase --continue
  rbet = rebase --edit-todo
  rbh = rebase -h
  rbi = rebase --interactive --committer-date-is-author-date
  rbq = rebase --quiet
  rbis = rebase --interactive --autostash --committer-date-is-author-date
  rbs = rebase --autostash --committer-date-is-author-date
  rbisq = rebase --interactive --autosquash --committer-date-is-author-date
  rbisqs = rebase --interactive --autosquash --autostash --committer-date-is-author-date
  rbsq = !GIT_SEQUENCE_EDITOR=true GIT_EDITOR=true git rebase --interactive --autosquash --committer-date-is-author-date
  rbsqs = !GIT_SEQUENCE_EDITOR=true GIT_EDITOR=true git rebase --interactive --autosquash --autostash --committer-date-is-author-date
  rbv = rebase --verbose
